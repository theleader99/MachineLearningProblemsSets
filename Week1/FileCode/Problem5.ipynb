   "source": [
    "def dp(s):\n",
    "  if s == s[::-1]:\n",
    "    return 0\n",
    "  elif s[0] == s[-1]:\n",
    "    return dp(s[1:-1])\n",
    "  else:\n",
    "    return 1+min(dp(s[1:]),dp(s[:-1]))\n",
    "\n",
    "  return dp(s)\n"
   ]
  },
